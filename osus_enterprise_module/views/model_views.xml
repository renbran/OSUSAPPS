<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="osus_main_model_view_form" model="ir.ui.view">
        <field name="name">osus.main.model.form</field>
        <field name="model">osus.main.model</field>
        <field name="arch" type="xml">
            <form string="Main Model">
                <header>
                    <field name="state" widget="statusbar"/>
                </header>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
