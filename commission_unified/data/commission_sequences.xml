<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Commission Lines Sequence -->
        <record id="sequence_commission_lines" model="ir.sequence">
            <field name="name">Commission Lines Sequence</field>
            <field name="code">commission.lines.unified</field>
            <field name="prefix">COMM-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_dependent">True</field>
        </record>

        <!-- Commission Rules Sequence -->
        <record id="sequence_commission_rules" model="ir.sequence">
            <field name="name">Commission Rules Sequence</field>
            <field name="code">commission.rules</field>
            <field name="prefix">RULE-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_dependent">True</field>
        </record>

        <!-- Commission Report Sequence -->
        <record id="sequence_commission_report" model="ir.sequence">
            <field name="name">Commission Report Sequence</field>
            <field name="code">commission.report</field>
            <field name="prefix">REP-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_dependent">True</field>
        </record>

    </data>
</odoo>