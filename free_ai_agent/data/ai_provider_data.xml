<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Sample AI Providers -->
    <record id="ai_provider_openai" model="ai.provider">
        <field name="name">OpenAI GPT-3.5</field>
        <field name="provider_type">openai</field>
        <field name="model_name">gpt-3.5-turbo</field>
        <field name="max_tokens">2000</field>
        <field name="temperature">0.7</field>
        <field name="sequence">10</field>
        <field name="active" eval="False"/>
    </record>

    <record id="ai_provider_openai_gpt4" model="ai.provider">
        <field name="name">OpenAI GPT-4</field>
        <field name="provider_type">openai</field>
        <field name="model_name">gpt-4</field>
        <field name="max_tokens">2000</field>
        <field name="temperature">0.7</field>
        <field name="sequence">5</field>
        <field name="active" eval="False"/>
    </record>

    <record id="ai_provider_claude" model="ai.provider">
        <field name="name">Anthropic Claude</field>
        <field name="provider_type">anthropic</field>
        <field name="model_name">claude-3-sonnet-20240229</field>
        <field name="max_tokens">2000</field>
        <field name="temperature">0.7</field>
        <field name="sequence">15</field>
        <field name="active" eval="False"/>
    </record>

    <record id="ai_provider_gemini" model="ai.provider">
        <field name="name">Google Gemini Pro</field>
        <field name="provider_type">google</field>
        <field name="model_name">gemini-pro</field>
        <field name="max_tokens">2000</field>
        <field name="temperature">0.7</field>
        <field name="sequence">20</field>
        <field name="active" eval="False"/>
    </record>

    <record id="ai_provider_ollama" model="ai.provider">
        <field name="name">Ollama Llama2 (Local)</field>
        <field name="provider_type">ollama</field>
        <field name="model_name">llama2</field>
        <field name="api_base_url">http://localhost:11434</field>
        <field name="max_tokens">2000</field>
        <field name="temperature">0.7</field>
        <field name="sequence">30</field>
        <field name="active" eval="False"/>
    </record>

</odoo>