<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Delete any problematic cached views -->
        <delete model="ir.ui.view" search="[('name', 'like', 'commission.enhanced'), ('model', '=', 'sale.order')]"/>
        <delete model="ir.ui.view" search="[('xmlid', 'like', 'commission_ax.view_order_form_commission_enhanced')]"/>
        <delete model="ir.ui.view" search="[('xmlid', '=', 'commission_ax.view_order_form_inherit_commission_enhanced_v2')]"/>

        <!-- Clear view cache to ensure fresh loading -->
        <function model="ir.ui.view" name="clear_caches">
            <value eval="()"/>
        </function>
    </data>
</odoo>
