<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Demo Commission Partners -->
        <record id="demo_broker_partner" model="res.partner">
            <field name="name">Demo Broker Partner</field>
            <field name="email">broker@demo.com</field>
            <field name="is_company">True</field>
            <field name="supplier_rank">1</field>
        </record>

        <record id="demo_referrer_partner" model="res.partner">
            <field name="name">Demo Referrer Partner</field>
            <field name="email">referrer@demo.com</field>
            <field name="is_company">True</field>
            <field name="supplier_rank">1</field>
        </record>

        <record id="demo_agent1_partner" model="res.partner">
            <field name="name">Demo Agent 1</field>
            <field name="email">agent1@demo.com</field>
            <field name="is_company">False</field>
            <field name="supplier_rank">1</field>
        </record>

        <record id="demo_manager_partner" model="res.partner">
            <field name="name">Demo Manager Partner</field>
            <field name="email">manager@demo.com</field>
            <field name="is_company">False</field>
            <field name="supplier_rank">1</field>
        </record>

        <!-- Demo Customer -->
        <record id="demo_customer" model="res.partner">
            <field name="name">Demo Customer with Reference</field>
            <field name="email">customer@demo.com</field>
            <field name="is_company">True</field>
            <field name="customer_rank">1</field>
        </record>

        <!-- Commission Product for Purchase Orders -->
        <record id="commission_service_product" model="product.product">
            <field name="name">Commission Service</field>
            <field name="detailed_type">service</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="list_price">0.0</field>
            <field name="standard_price">0.0</field>
            <field name="sale_ok">False</field>
            <field name="purchase_ok">True</field>
            <field name="description_sale">Service product for commission payments</field>
        </record>
    </data>
</odoo>
