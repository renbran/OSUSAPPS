        <!-- Demo Service Providers -->
        <record id="demo_service_provider_1" model="res.users">
            <field name="name">Dr. Sarah Johnson</field>
            <field name="login">dr.sarah.johnson</field>
            <field name="email">sarah.johnson@osusapps.com</field>
            <field name="tz">America/New_York</field>
            <field name="groups_id" eval="[(4, ref('base.group_user'))]"/>
        </record>
        
