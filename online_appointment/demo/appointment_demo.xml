        <!-- Demo Appointment Options -->
        <record id="demo_option_consultation" model="online_appointment.option">
            <field name="name">General Consultation</field>
            <field name="description">General consultation session for business needs assessment</field>
            <field name="duration">1.0</field>
            <field name="active" eval="True"/>
            <field name="website_published" eval="True"/>
            <field name="user_specific" eval="False"/>
        </record>
        
