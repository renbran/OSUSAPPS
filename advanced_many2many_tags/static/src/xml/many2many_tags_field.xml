<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <!--Template to show when clicked on the many2many field-->
    <t t-name="advanced_many2many_tags.SaveFormDialog"
       t-inherit="web.ConfirmationDialog" t-inherit-mode="extension">
        <xpath expr="//button[hasclass('btn')]" position="after">
            <button class="btn btn-secondary"
                    t-att-class="props.open_form_viewClass"
                    t-on-click="_open_form_view">Open form View
            </button>
        </xpath>
    </t>
</templates>
