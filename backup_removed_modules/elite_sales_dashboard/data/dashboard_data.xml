<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron Job for Commission Sync -->
        <record id="ir_cron_commission_sync" model="ir.cron">
            <field name="name">Sync Commission Data</field>
            <field name="model_id" ref="model_elite_commission_sync"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_commissions()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
        
        <!-- Cron Job for Dashboard Data Updates -->
        <record id="ir_cron_dashboard_update" model="ir.cron">
            <field name="name">Update Dashboard Data</field>
            <field name="model_id" ref="model_elite_agent_dashboard"/>
            <field name="state">code</field>
            <field name="code">model._update_dashboard_data()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
    </data>
</odoo>