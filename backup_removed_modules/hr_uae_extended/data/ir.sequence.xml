<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Air Tickets -->
        <record id="seq_hr_air_ticket" model="ir.sequence">
            <field name="name">Air Ticket Sequence</field>
            <field name="code">hr.air.ticket</field>
            <field name="prefix">TKT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
