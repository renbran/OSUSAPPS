<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Commission Allocation Sequence -->
        <record id="seq_commission_allocation" model="ir.sequence">
            <field name="name">Commission Allocation</field>
            <field name="code">commission.allocation</field>
            <field name="prefix">CA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Commission Period Sequence -->
        <record id="seq_commission_period" model="ir.sequence">
            <field name="name">Commission Period</field>
            <field name="code">commission.period</field>
            <field name="prefix">CP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Commission Rule Sequence -->
        <record id="seq_commission_rule" model="ir.sequence">
            <field name="name">Commission Rule</field>
            <field name="code">commission.rule</field>
            <field name="prefix">CR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>