<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron Job for Scheduled Invitations -->
        <record id="cron_send_scheduled_invitations" model="ir.cron">
            <field name="name">Send Scheduled Meeting Invitations</field>
            <field name="model_id" ref="model_calendar_announcement"/>
            <field name="state">code</field>
            <field name="code">model._cron_send_scheduled_invitations()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
